      <!-- CommerceAI -->
      <section class="commerce">
        <div class="commerce-wrapper">
          <h3 class="commerce-title">Qubit CommerceAI.</h3>
          <p class="post-title">The new way to drive growth.</p>
          <div class="commerce-cards">
            <div class="commerce-card">
              <div class="commerce-card-pic">
                <img src="/img/svg/more-insight-thumb.svg" alt="More insight" />
              </div>
              <h4 class="commerce-card-title">More insight</h4>
              <p class="commerce-card-desc">
                Learn which products drive performance and which don't, through
                automated insights you can action.
              </p>
            </div>
            <div class="commerce-card">
              <div class="commerce-card-pic">
                <img
                  src="/img/svg/more-personal-thumb.svg"
                  alt="More personal"
                />
              </div>
              <h4 class="commerce-card-title">More personal</h4>
              <p class="commerce-card-desc">
                Qubit CommerceAI generates a model per shopper to make every
                customer interaction more relevant.
              </p>
            </div>
            <div class="commerce-card">
              <div class="commerce-card-pic">
                <img
                  src="/img/svg/more-effective-thumb.svg"
                  alt="More effective"
                />
              </div>
              <h4 class="commerce-card-title">More effective</h4>
              <p class="commerce-card-desc">
                Qubit fuses data, intent, and design tools to enable your team
                to create more effective customer experiences.
              </p>
            </div>
          </div>
        </div>
